{
  "name": "rosenichols",
  "version": "1.0.0",
  "author": "Rose Nichols",
  "private": true,
  "scripts": {
    "autoprefixer": "postcss -u autoprefixer -r public/css/*.css",
    "scss": "node-sass --output-style compressed -o public/css public/scss",
    "build": "npm run scss && npm run autoprefixer"
  },
  "devDependencies": {
    "autoprefixer": "^9.6.0",
    "node-sass": "^4.14.1",
    "postcss-cli": "^6.1.3"
  },
  "engines": {
    "node": ">=6 <=10"
  }
}
