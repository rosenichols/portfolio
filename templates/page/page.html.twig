<!doctype html>
<html lang="en-gb">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Rose Nichols, freelance web development</title>
    <meta name="description" content="The website of Rose Nichols, experienced freelance web developer specialising in Drupal, and Twig templating for any platform." />

    <link rel="apple-touch-icon" sizes="180x180" href="/apple-touch-icon.png">
    <link rel="icon" type="image/png" sizes="32x32" href="/favicon-32x32.png">
    <link rel="icon" type="image/png" sizes="16x16" href="/favicon-16x16.png">
    <link rel="manifest" href="/site.webmanifest">
    <link rel="mask-icon" href="/safari-pinned-tab.svg" color="#3c896d">
    <meta name="msapplication-TileColor" content="#3c896d">
    <meta name="theme-color" content="#353d46">

    <meta property="og:site_name" content="Rose Nichols" />

    <link rel="preload" href="/fonts/overpass/overpass-regular-webfont.woff2" as="font" type="font/woff2" crossorigin>
    <link rel="preload" href="/fonts/rubik/rubik-medium.woff2" as="font" type="font/woff2" crossorigin>
    <link href="/css/rose.css" rel="stylesheet">
    {% block stylesheets %}
    {% endblock %}
    {% block header_js %}
    {% endblock %}
  </head>
  <body class="font:overpass {% block bodyClass %}{% endblock %}">

    {% include 'partials/svg.html.twig' %}

    {% block header %}
      <header class="site-header bg:green-yellow-zigzag pad-b:18">
        <div class="bg:blue-grey-850 pad-v:2">
          <div class="container wl-container mar-h:auto">
            <svg class="fill:off-white h:10" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 228 55">
              <use xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="#logo"></use>
            </svg>
          </div>
        </div>
      </header>
    {% endblock %}

    <main class="site-body">
      {% if app.session.getFlashBag.peekAll()|length %}
        <div class="alerts">
          {% for message in app.session.getFlashBag.get('message.success') %}
            <div data-alert class="alert-box alert-type-success">{{ message|raw }}</div>
          {% endfor %}
          {% for message in app.session.getFlashBag.get('message.alert') %}
            <div data-alert class="alert-box alert-type-alert">{{ message }}</div>
          {% endfor %}
        </div>
      {% endif %}
      {% block content %}
        {{ content|raw }}
      {% endblock %}
    </main>

    {% block footer %}
      <footer class="bg:blue-grey-900">
        <div class="container wl-container mar-h:auto pad-v:8">
          <div class="pos:rel grid l:grid-cols:2 grid-gap:10 xl:grid-gap:12 xxl:grid-gap:18 pad-v:6 pad-t:18">
